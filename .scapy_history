import IncrementalClass
import IncrementalClass.py
class Incremental(object):
        def __init__(self, max):
                self.max = max
                self.c = 0
        def __iter__(self):
                self.c = 0
                return self
        def __next__(self):
                if self.c > self.max:
                        raise StopIteration
                self.c = self.c + 1
                return self.c
        next = __next__
import IncrementClass
import Incremental
import IncrementalClass
import Incremental
sys.path
sys.path.append('.')
